{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "tslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "tslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "ngrx-sample",
  "projects": {
    "ngrx-sample": {
      "projectType": "application",
      "root": "apps/ngrx-sample",
      "sourceRoot": "apps/ngrx-sample/src",
      "prefix": "jl",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ngrx-sample",
            "index": "apps/ngrx-sample/src/index.html",
            "main": "apps/ngrx-sample/src/main.ts",
            "polyfills": "apps/ngrx-sample/src/polyfills.ts",
            "tsConfig": "apps/ngrx-sample/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ngrx-sample/src/favicon.ico",
              "apps/ngrx-sample/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/ngrx-sample/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ngrx-sample/src/environments/environment.ts",
                  "with": "apps/ngrx-sample/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngrx-sample:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngrx-sample:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngrx-sample:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ngrx-sample/tsconfig.app.json",
              "apps/ngrx-sample/tsconfig.spec.json",
              "apps/ngrx-sample/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/ngrx-sample/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ngrx-sample"],
          "options": {
            "jestConfig": "apps/ngrx-sample/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ngrx-sample-e2e": {
      "root": "apps/ngrx-sample-e2e",
      "sourceRoot": "apps/ngrx-sample-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ngrx-sample-e2e/cypress.json",
            "tsConfig": "apps/ngrx-sample-e2e/tsconfig.e2e.json",
            "devServerTarget": "ngrx-sample:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngrx-sample:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/ngrx-sample-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/ngrx-sample-e2e/**/*"]
          }
        }
      }
    },
    "core-data": {
      "projectType": "library",
      "root": "libs/core-data",
      "sourceRoot": "libs/core-data/src",
      "prefix": "jl",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/core-data/tsconfig.lib.json",
              "libs/core-data/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/core-data/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core-data"],
          "options": {
            "jestConfig": "libs/core-data/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core-state": {
      "projectType": "library",
      "root": "libs/core-state",
      "sourceRoot": "libs/core-state/src",
      "prefix": "jl",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/core-state/tsconfig.lib.json",
              "libs/core-state/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/core-state/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core-state"],
          "options": {
            "jestConfig": "libs/core-state/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "material": {
      "projectType": "library",
      "root": "libs/material",
      "sourceRoot": "libs/material/src",
      "prefix": "jl",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/material/tsconfig.lib.json",
              "libs/material/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/material/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/material"],
          "options": {
            "jestConfig": "libs/material/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "node-app": {
      "root": "apps/node-app",
      "sourceRoot": "apps/node-app/src",
      "projectType": "application",
      "prefix": "node-app",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/node-app",
            "main": "apps/node-app/src/main.ts",
            "tsConfig": "apps/node-app/tsconfig.app.json",
            "assets": ["apps/node-app/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/node-app/src/environments/environment.ts",
                  "with": "apps/node-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "node-app:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/node-app/tsconfig.app.json",
              "apps/node-app/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/node-app/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/node-app"],
          "options": {
            "jestConfig": "apps/node-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
